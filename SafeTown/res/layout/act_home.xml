<?xml version="1.0" encoding="utf-8"?>
<LinearLayout style="@style/Fill" xmlns:a="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res/com.saferoute">
	<com.saferoute.HeaderBar style="@style/Fill.Width" a:layout_height="@dimen/actionbar_div_height" a:id="@+id/actionbar_div" />
		<LinearLayout a:layout_width="fill_parent"
		a:gravity="center_horizontal" a:id="@+id/header_layout"
		a:orientation="vertical" a:layout_height="wrap_content">
		<org.taptwo.android.widget.TitleFlowIndicator
			a:id="@+id/viewflowindic" a:layout_height="wrap_content"
			a:layout_width="fill_parent"
			app:footerLineHeight="2dp"
			app:footerTriangleHeight="10dp" app:textColor="#FFFFFFFF" app:selectedColor="#FFFFC445" app:footerColor="#FFFFC445" app:titlePadding="10dp" app:textSize="11dp" app:selectedSize="12dp" a:layout_marginTop="10dip" app:clipPadding="5dp" />

	</LinearLayout>
	<org.taptwo.android.widget.ViewFlow
		a:duplicateParentState="true" a:id="@+id/viewflow"
		a:layout_width="fill_parent" a:layout_height="200dp" />
	<com.google.android.maps.MapView xmlns:android="http://schemas.android.com/apk/res/android"
			a:id="@+id/mapview"
			a:layout_width="fill_parent"
			a:layout_height="fill_parent"
			a:clickable="true"
			a:apiKey="0T-y8hGgXs568h7TcnF-7dpTzheQZUSk7t3ocyg" />

		<LinearLayout xmlns:a="http://schemas.android.com/apk/res/android"
		    a:orientation="vertical"
		    a:layout_width="100dp"
		    a:layout_height="100dp" >
				<CheckBox a:id="@+id/checkbox"
		        a:layout_width="wrap_content"
		        a:layout_height="wrap_content"
		        a:text="check" />
		        <CheckBox a:id="@+id/checkbox"
		        a:layout_width="wrap_content"
		        a:layout_height="wrap_content"
		        a:text="it" />
		        <CheckBox a:id="@+id/checkbox"
		        a:layout_width="wrap_content"
		        a:layout_height="wrap_content"
		        a:text="out" />


		</LinearLayout>
</LinearLayout>